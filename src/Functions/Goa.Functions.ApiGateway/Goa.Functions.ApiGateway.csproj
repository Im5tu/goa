<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <PackageTitle>Goa - API Gateway Functions</PackageTitle>
        <Description>API Gateway integration with V1/V2 payload support for high-performance AWS Lambda functions</Description>
        <PackageTags>$(PackageBaseTags);function;apigateway;http;rest</PackageTags>
    </PropertyGroup>

    <PropertyGroup>
        <OutputType>Library</OutputType>
    </PropertyGroup>

    <ItemGroup>
        <FrameworkReference Include="Microsoft.AspNetCore.App" />
        <PackageReference Include="System.IdentityModel.Tokens.Jwt" Version="8.14.0" />
    </ItemGroup>

</Project>
