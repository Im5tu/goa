{
    "$schema": "http://json.schemastore.org/template",
    "author": "Goa",
    "classifications": [ "AWS", "Lambda", "Serverless", "Goa" ],
    "identity": "Goa.Templates.Sqs",
    "name": "Lambda - SQS Function",
    "shortName": "goa.sqs",
    "sourceName": "AwsSqs",
    "tags": {
        "language": "C#",
        "type": "project"
    },
    "symbols": {
        "processingType": {
            "type": "parameter",
            "description": "The type of SQS message processing to use.",
            "datatype": "choice",
            "choices": [
                {
                    "choice": "single",
                    "description": "Process SQS messages one at a time"
                },
                {
                    "choice": "batch",
                    "description": "Process SQS messages as a batch (Default)"
                }
            ],
            "replaces": "processingType",
            "defaultValue": "batch"
        }
    }
}
