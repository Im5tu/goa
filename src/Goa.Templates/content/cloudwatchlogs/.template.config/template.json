{
    "$schema": "http://json.schemastore.org/template",
    "author": "Goa",
    "classifications": [ "AWS", "Lambda", "Serverless", "Goa" ],
    "identity": "Goa.Templates.CloudWatchLogs",
    "name": "Lambda - CloudWatch Logs Function",
    "shortName": "goa.cloudwatchlogs",
    "sourceName": "AwsCloudWatchLogs",
    "tags": {
        "language": "C#",
        "type": "project"
    },
    "symbols": {
        "processingType": {
            "type": "parameter",
            "description": "The type of log event processing to use.",
            "datatype": "choice",
            "choices": [
                {
                    "choice": "single",
                    "description": "Process log events one at a time"
                },
                {
                    "choice": "batch",
                    "description": "Process log events as a batch (Default)"
                }
            ],
            "replaces": "processingType",
            "defaultValue": "batch"
        }
    }
}
