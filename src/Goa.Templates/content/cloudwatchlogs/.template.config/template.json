{
    "$schema": "http://json.schemastore.org/template",
    "author": "Goa",
    "classifications": [ "AWS", "Lambda", "Serverless", "Goa" ],
    "identity": "Goa.Templates.CloudWatchLogs",
    "name": "Lambda - CloudWatch Logs Function",
    "shortName": "goa.cloudwatchlogs",
    "sourceName": "AwsCloudWatchLogs",
    "tags": {
        "language": "C#",
        "type": "project"
    },
    "symbols": {
        "processingType": {
            "type": "parameter",
            "description": "How to handle CloudWatch control messages.",
            "datatype": "choice",
            "choices": [
                {
                    "choice": "withoutControlMessages",
                    "description": "Skip control messages (connectivity checks are filtered out)"
                },
                {
                    "choice": "withControlMessages",
                    "description": "Include control messages (handler receives all messages)"
                }
            ],
            "replaces": "processingType",
            "defaultValue": "withoutControlMessages"
        }
    }
}
